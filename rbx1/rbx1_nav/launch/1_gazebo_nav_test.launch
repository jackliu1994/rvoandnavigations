<launch>
  <!-- Map server -->
  <arg name="map_file" default="$(env TURTLEBOT_GAZEBO_MAP_FILE)"/>
  <arg name="3d_sensor" default="$(env TURTLEBOT_3D_SENSOR)"/>  <!-- r200, kinect, asus_xtion_pro -->

  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" >
      <param name="frame_id" value="map" />
</node>
  <!--Localization -->
  <node pkg="fake_localization" type="fake_localization" name="fake_localization" clear_params="true" output="screen">
     <remap from="base_pose_ground_truth" to="odom" />
    
     <param name="odom_frame_id" value="$(arg __ns)/odom" />
     <param name="global_frame_id" value="$(arg __ns)/map" />
     <param name="base_frame_id" value="$(arg __ns)/base_footprint" />
  </node>


  <!-- Move base -->
 <include file="$(find rbx1_nav)/launch/1_gazebo_movebase.launch"/>
</launch>

